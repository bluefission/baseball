{
    "name": "bluefission/baseball",
    "type": "project",
    "description": "Baseball Stats.",
    "keywords": [
        "baseball",
        "codetest"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Devon Scott",
            "email": "dscott@bluefission.com",
            "homepage": "http://bluefission.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bluefission/develation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bluefission/automata"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "bluefission/develation": "^1.0.0-alpha",
        "bluefission/automata": "dev-master",
        "symfony/http-client": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1@dev",
        "php": ">=8.0.0"
    },
    "autoload": {
        "psr-4": {
            "BlueFission\\baseball\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "BlueFission\\baseball\\Tests\\": "tests/" 
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
